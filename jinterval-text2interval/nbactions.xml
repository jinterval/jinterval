<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-ea -Djna.library.path=. -classpath %classpath net.java.jinterval.text2interval.gen.GenP1788</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>../wrk</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -ea -Djna.library.path=. -classpath %classpath net.java.jinterval.text2interval.gen.GenP1788</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>../wrk</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-ea -Djna.library.path=. -classpath %classpath net.java.jinterval.text2interval.gen.GenP1788</exec.args>
                <exec.executable>${profiler.java}</exec.executable>
                <exec.workingdir>../wrk</exec.workingdir>
            </properties>
        </action>
    </actions>
